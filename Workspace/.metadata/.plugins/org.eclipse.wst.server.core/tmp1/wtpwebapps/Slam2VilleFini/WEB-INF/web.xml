<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<display-name>Slam2Ville</display-name>
	<description>Slam2Ville</description>

	<servlet>
		<servlet-name>RechercheVilleJson</servlet-name>
		<servlet-class>fr.pfit.sio.villesajax.servlets.RechercheVilleJson</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>RechercheVille</servlet-name>
		<display-name>RechercheVille</display-name>
		<description></description>
		<servlet-class>fr.pfit.sio.villesajax.servlets.RechercheVille</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RechercheVilleJson</servlet-name>
		<url-pattern>/RechercheVilleJson</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RechercheVille</servlet-name>
		<url-pattern>/RechercheVille</url-pattern>
	</servlet-mapping>
</web-app>
